<package>
  <name>wire_viz</name>
  <version>0.0.1</version>
  <description>
     Package that visualizes WIRE World Model input and output in RViz using visualization markers
  </description>
  <maintainer email="j.elfring@tue.nl">Jos Elfring, Sjoerd van den Dries</maintainer>
  <license>BSD</license>
  <url type="website">http://ros.org/wiki/wire_viz</url>
  
  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>wire_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>visualization_msgs</build_depend>
  <build_depend>problib</build_depend>
  <build_depend>tf</build_depend>

  <run_depend>roscpp</run_depend>
  <run_depend>wire_msgs</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>visualization_msgs</run_depend>
  <run_depend>problib</run_depend>
  <run_depend>tf</run_depend>

</package>


